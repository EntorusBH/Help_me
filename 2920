#include <stdio.h>

int main() {
	int arr[8];
	int asc, dsc = 0;
	for (int i=0;i<8;i++) {
		scanf("%d", &arr[i]);
		if (arr[i]==i+1) {
			asc++;
		}
		else if (arr[i]==8-i) {
			dsc++;
		}
	}
	if (asc==8) {
		printf("ascending"); // 왜 1~8 순차적으로 입력해도 mixed가 나올까
	}
	else if (dsc==8) {
		printf("descending");
	}
	else {
		printf("mixed");
	}
	return 0;
}
